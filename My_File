when(parameterUtil.getEnvironmentParameter("reflecktions_api_key"))
        .thenReturn("dummy-api-key");

when(parameterUtil.getEnvironmentParameter("reflecktions_customer_key"))
        .thenReturn("dummy-customer-key");

when(parameterUtil.getEnvironmentParameter("sfcc_shop_client_id"))
        .thenReturn("dummy-client-id");
